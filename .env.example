# .env

# Freeseek API Key
FREESEEK_API_KEY=your_api_key_here

# Base URL for the Freeseek API
FREESEEK_BASE_URL=https://api.freeseek.com/v1

# Timeout for API requests in seconds
FREESEEK_TIMEOUT=30

# Maximum number of retries for failed requests
FREESEEK_MAX_RETRIES=3

# Backoff factor for retrying failed requests
FREESEEK_BACKOFF_FACTOR=1

# Logging configuration
FREESEEK_LOG_LEVEL=INFO
FREESEEK_LOG_FILE=logs/freeseek.log

# Middleware configuration (if any)
FREESEEK_MIDDLEWARE_PRE_REQUEST=path.to.pre_request_middleware
FREESEEK_MIDDLEWARE_POST_RESPONSE=path.to.post_response_middleware